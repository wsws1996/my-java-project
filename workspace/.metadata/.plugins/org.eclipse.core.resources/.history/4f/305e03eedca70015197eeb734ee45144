public class Demo3 {

	/*
	 * 
	 * create table testbench ( id int primary key, name varchar(20) )
	 */

	public void testbench1() {
		Connection connection = null;
		Statement statement = null;
		try {
			connection = JdbcUtils.getConnection();
			String sql1 = "insert into testbench (id,name) values(1,'aaa')";
			String sql2 = "insert into testbench (id,name) values(2,'bbb')";
			String sql3 = "delete from testbench where id=1";
			
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			JdbcUtils.release(connection, preparedStatement, null);
		}
	}

}
